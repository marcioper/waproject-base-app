{
  "name": "reactApp",
  "version": "1.0.0",
  "versionBuild": 1,
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "lint": "yarn eslint \"./src/**/*.ts\" \"./src/**/*.tsx\"",
    "dev-android": "yarn run react-native run-android",
    "dev-ios": "yarn run react-native run-ios",
    "release-android": "sh scripts/release.android.sh",
    "release-android-fastlane": "(cd ./android && fastlane release)",
    "release-ios": "sh scripts/release.ios.sh",
    "release-ios-fastlane": "(cd ./ios && fastlane release)"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently -r \"yarn lint\" \"yarn tsc --noEmit\"",
      "post-merge": "yarn"
    }
  },
